#include <stdio.h>

void max_index(int *v, int *n){
int aux = 0;
    for(int i = 1; i < *n; i++){
      if(*(v + i) > *(v + aux)){
        aux = i;
      }
    }

  printf("O número de maior índice é :%d", aux+1);  
}

int main(void){

  int vetor[100], n;

  printf("Digite o tamanho do vetor:\n");
  scanf("%d", &n);

  printf("Digite os números do vetor:\n");
  for(int i = 0; i < n; i++){
    scanf("%d", &vetor[i]);
  }

  max_index(vetor, &n);
  
  return 0;
}
